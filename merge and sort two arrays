#include<bits/stdc++.h>
#define ll long long int
using namespace std;
int32_t main()
{
    ll a[]={1,4,7,8,10};
    ll b[]={2,3,9};
    int m=sizeof(a)/sizeof(a[0]);
    int n=sizeof(b)/sizeof(b[0]);
    sort(a,a+m);
    sort(b,b+n);
    ll ptr1=a[0];
    ll ptr2=b[0];
    for(ll i=0;i<m;i++)
    {
        if(a[i]>b[0])
        {
            swap(a[i],b[0]);
            sort(b,b+n);
        }
    }
    for(ll i=0;i<m;i++)
    cout<<a[i]<<" ";
    cout<<endl;
    for(ll i=0;i<n;i++)
    cout<<b[i]<<" ";
    return 0;
}
